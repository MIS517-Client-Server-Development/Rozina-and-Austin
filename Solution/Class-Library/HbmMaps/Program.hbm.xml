<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="BusinessLogic.Layer.BusinessObjects" assembly="BusinessLogic.Layer">
	<class name="BusinessLogic.Layer.BusinessObjects.Program, BusinessLogic.Layer" table="[dbo].[Program]" lazy="true">
		<id name="Id" column="[ID]">
		 	<generator class="native" />
		</id>
		<property name="ProgramTitle" column="[ProgramTitle]" />
		<property name="ProgramDesc" column="[ProgramDesc]" />
		<bag name="SchYrSems" lazy="true" cascade="all" inverse="true" >
			<key column="[ProgramID]"></key>
			<one-to-many class="SchYrSem"></one-to-many>
		</bag>
	</class>
</hibernate-mapping>

